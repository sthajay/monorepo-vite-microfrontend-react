{
  "private": true,
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "dev:movies": "cd apps/movies && pnpm run dev",
    "dev:playlist": "cd apps/playlist && pnpm run dev"
  },
  "devDependencies": {
    "eslint": "^7.32.0",
    "eslint-config-custom": "workspace:*",
    "prettier": "^2.5.1",
    "turbo": "^1.10.12"
  },
  "packageManager": "pnpm@7.15.0",
  "name": "monorepo-vite-microfrontend-react",
  "workspaces": [
    "apps/*",
    "packages/*"
  ]
}
